<%- include('partials/header'); -%> <% posts.forEach(function(post){ %>
<h1><%= post.blogTitle %></h1>
<p>
	<%= post.blogBody.length <= 100 ? post.blogBody : (post.blogBody .substr(0,
	100)+"...") %>

	<a href="post/<%= post.blogTitle %>">Read More</a>
</p>

<% }) %> <%-include('partials/footer'); -%>
